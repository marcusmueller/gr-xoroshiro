<?xml version="1.0"?>
<block>
  <name>XOROSHIRO128+ Source</name>
  <key>xoroshiro_xoroshiro128p_source</key>
  <category>[xoroshiro]</category>
  <import>import xoroshiro</import>
  <make>xoroshiro.xoroshiro128p_source($seed, $iterations)</make>
  <param>
    <name>Seed</name>
    <key>seed</key>
    <type>int</type>
  </param>
  <param>
    <name>Iterations</name>
    <key>iterations</key>
    <type>int</type>
  </param>
  <!-- Make one 'source' node per output. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <source>
    <name>out</name>
    <type>float</type>
  </source>
</block>
